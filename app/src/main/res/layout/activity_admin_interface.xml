<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/layout"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Admin.ADMIN_INTERFACE">

    <Button
        android:id="@+id/interface_Add_Work"
        android:layout_width="164dp"
        android:layout_height="60dp"
        android:layout_marginTop="16dp"
        android:text="Abrechnung"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.528"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/Image" />

    <Button
        android:id="@+id/Abrechnung"
        android:layout_width="171dp"
        android:layout_height="60dp"
        android:layout_marginTop="28dp"
        android:text="ADD Work"
        app:layout_constraintEnd_toEndOf="@+id/interface_Add_Work"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/interface_Add_Work"
        app:layout_constraintTop_toBottomOf="@+id/interface_Add_Work" />

    <Button
        android:id="@+id/logout"
        android:layout_width="171dp"
        android:layout_height="60dp"
        android:layout_marginTop="112dp"
        android:text="logout"
        app:layout_constraintEnd_toEndOf="@+id/interface_Add_Work"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/interface_Add_Work"
        app:layout_constraintTop_toBottomOf="@+id/interface_Add_Work" />

    <ImageView
        android:id="@+id/Image"
        android:layout_width="385dp"
        android:layout_height="438dp"
        android:layout_marginStart="4dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.16"
        app:srcCompat="@drawable/open" />

</androidx.constraintlayout.widget.ConstraintLayout>