<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".SignUP.Login_Interface">

    <Button
        android:id="@+id/btnStudentInt"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="36dp"
        android:text="Student Login"
        app:layout_constraintEnd_toEndOf="@+id/imageView"
        app:layout_constraintStart_toStartOf="@+id/imageView"
        app:layout_constraintTop_toBottomOf="@+id/imageView" />

    <Button
        android:id="@+id/btnAdministratorInt"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="24dp"
        android:text="Administrator Login"
        app:layout_constraintEnd_toEndOf="@+id/btnProfessorInt"
        app:layout_constraintHorizontal_bias="0.574"
        app:layout_constraintStart_toStartOf="@+id/btnProfessorInt"
        app:layout_constraintTop_toBottomOf="@+id/btnProfessorInt" />

    <Button
        android:id="@+id/btnProfessorInt"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="24dp"
        android:text="Professor Login"
        app:layout_constraintEnd_toEndOf="@+id/btnStudentInt"
        app:layout_constraintHorizontal_bias="0.473"
        app:layout_constraintStart_toStartOf="@+id/btnStudentInt"
        app:layout_constraintTop_toBottomOf="@+id/btnStudentInt" />

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="597dp"
        android:layout_height="676dp"
        android:layout_marginTop="16dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.39"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/login_interface" />

</androidx.constraintlayout.widget.ConstraintLayout>