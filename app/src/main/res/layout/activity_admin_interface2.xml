<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Admin.ADMIN_INTERFACE_2">

    <ImageView
        android:id="@+id/imageView11"
        android:layout_width="717dp"
        android:layout_height="497dp"
        android:layout_marginTop="44dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/professor_login" />

    <Button
        android:id="@+id/btn_Delete_User"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="52dp"
        android:text="Delete User"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView11" />

    <Button
        android:id="@+id/btn_Delete_Work"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="28dp"
        android:text="Delete Work"
        app:layout_constraintEnd_toEndOf="@+id/btn_Delete_User"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="@+id/btn_Delete_User"
        app:layout_constraintTop_toBottomOf="@+id/btn_Delete_User" />

    <Button
        android:id="@+id/Interface3"
        android:layout_width="wrap_content"
        android:layout_height="0dp"
        android:layout_marginTop="28dp"
        android:text="Interface 3"
        app:layout_constraintEnd_toEndOf="@+id/btn_Delete_Work"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="@+id/btn_Delete_Work"
        app:layout_constraintTop_toBottomOf="@+id/btn_Delete_Work" />

    <Button
        android:id="@+id/Interface2"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="28dp"
        android:text="Interface 1"
        app:layout_constraintEnd_toEndOf="@+id/Interface3"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="@+id/Interface3"
        app:layout_constraintTop_toBottomOf="@+id/Interface3" />
</androidx.constraintlayout.widget.ConstraintLayout>