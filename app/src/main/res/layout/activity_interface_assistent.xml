<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Assistent.INTERFACE_Assistent">


    <Button
        android:id="@+id/Assigned_Students"
        android:layout_width="130dp"
        android:layout_height="60dp"
        android:layout_marginTop="36dp"
        android:text="Assigned Students"
        app:layout_constraintEnd_toEndOf="@+id/imageView7"
        app:layout_constraintHorizontal_bias="0.502"
        app:layout_constraintStart_toStartOf="@+id/imageView7"
        app:layout_constraintTop_toBottomOf="@+id/imageView7" />

    <Button
        android:id="@+id/changeSlot"
        android:layout_width="130dp"
        android:layout_height="60dp"
        android:layout_marginTop="112dp"
        android:text="Change slot"
        app:layout_constraintEnd_toEndOf="@+id/Assigned_Students"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/Assigned_Students"
        app:layout_constraintTop_toBottomOf="@+id/Assigned_Students" />

    <ImageView
        android:id="@+id/imageView7"
        android:layout_width="400dp"
        android:layout_height="297dp"
        android:layout_marginTop="28dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.454"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/student_interface" />

</androidx.constraintlayout.widget.ConstraintLayout>