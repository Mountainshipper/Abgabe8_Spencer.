<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Admin.Interface_Work_ADD"
    tools:layout_editor_absoluteX="16dp"
    tools:layout_editor_absoluteY="-273dp">

    <CheckBox
        android:id="@+id/chack_Project"
        android:layout_width="100dp"
        android:layout_height="wrap_content"
        android:text="Project"
        app:layout_constraintBottom_toBottomOf="@+id/check_Bachelor"
        app:layout_constraintEnd_toStartOf="@+id/check_Bachelor"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/check_Bachelor" />

    <CheckBox
        android:id="@+id/check_Bachelor"
        android:layout_width="100dp"
        android:layout_height="wrap_content"
        android:text="Bachelor"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.09" />

    <CheckBox
        android:id="@+id/check_Master"
        android:layout_width="100dp"
        android:layout_height="wrap_content"
        android:text="Master"
        app:layout_constraintBottom_toBottomOf="@+id/check_Bachelor"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/check_Bachelor"
        app:layout_constraintTop_toTopOf="@+id/check_Bachelor" />

    <EditText
        android:id="@+id/txt_Titel"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:ems="10"
        android:hint="Titel"
        android:inputType="textPersonName"
        android:minHeight="48dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.497"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.234" />

    <EditText
        android:id="@+id/txt_deadline"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="32dp"
        android:ems="10"
        android:hint="Deadline"
        android:inputType="textPersonName"
        android:minHeight="48dp"
        app:layout_constraintEnd_toEndOf="@+id/txt_Titel"
        app:layout_constraintStart_toStartOf="@+id/txt_Titel"
        app:layout_constraintTop_toBottomOf="@+id/txt_Titel" />

    <EditText
        android:id="@+id/txtExamDate"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="32dp"
        android:ems="10"
        android:hint="Exam Date. (Master)"
        android:inputType="textPersonName"
        android:minHeight="48dp"
        app:layout_constraintEnd_toEndOf="@+id/txt_deadline"
        app:layout_constraintStart_toStartOf="@+id/txt_deadline"
        app:layout_constraintTop_toBottomOf="@+id/txt_deadline" />

    <ImageView
        android:id="@+id/imageView12"
        android:layout_width="342dp"
        android:layout_height="204dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/txtExamDate"
        app:layout_constraintStart_toStartOf="@+id/txtExamDate"
        app:layout_constraintTop_toBottomOf="@+id/txtExamDate"
        app:layout_constraintVertical_bias="0.899"
        app:srcCompat="@drawable/admin_work" />

    <Button
        android:id="@+id/btn_setWork"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="32dp"
        android:text="Upload"
        app:layout_constraintBottom_toTopOf="@+id/imageView12"
        app:layout_constraintEnd_toEndOf="@+id/txtExamDate"
        app:layout_constraintHorizontal_bias="0.47"
        app:layout_constraintStart_toStartOf="@+id/txtExamDate"
        app:layout_constraintTop_toBottomOf="@+id/txtExamDate"
        app:layout_constraintVertical_bias="0.777" />

</androidx.constraintlayout.widget.ConstraintLayout>