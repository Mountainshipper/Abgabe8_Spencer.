<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Admin.Start">

    <LinearLayout
        android:layout_width="391dp"
        android:layout_height="78dp"
        android:layout_marginTop="4dp"
        android:orientation="horizontal"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <ImageView
            android:id="@+id/imageView3"
            android:layout_width="106dp"
            android:layout_height="76dp"
            app:srcCompat="@drawable/image" />

        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="286dp"
            android:layout_height="80dp">

            <TextView
                android:id="@+id/textView3"
                android:layout_width="107dp"
                android:layout_height="31dp"
                android:text="Add Bill"
                android:textColor="#000000"
                android:textSize="24sp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.312"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintVertical_bias="0.51" />

            <ImageView
                android:id="@+id/imageView5"
                android:layout_width="56dp"
                android:layout_height="48dp"
                app:srcCompat="@drawable/buttton"
                tools:layout_editor_absoluteX="214dp"
                tools:layout_editor_absoluteY="16dp" />
        </androidx.constraintlayout.widget.ConstraintLayout>

    </LinearLayout>

    <com.google.android.material.floatingactionbutton.FloatingActionButton
        android:id="@+id/floatingActionButton3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="40dp"
        android:clickable="true"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.916"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@drawable/ic_baseline_home_24" />

    <LinearLayout
        android:layout_width="389dp"
        android:layout_height="446dp"
        android:orientation="vertical"
        app:layout_constraintEnd_toEndOf="@+id/linearLayout"
        app:layout_constraintStart_toStartOf="@+id/linearLayout"
        app:layout_constraintTop_toBottomOf="@+id/linearLayout">

        <View
            android:layout_width="match_parent"
            android:layout_height="2dp"
            android:background="#000000" />

        <TextView
            android:id="@+id/tv_showbills"
            android:layout_width="match_parent"
            android:layout_height="11dp" />

    </LinearLayout>

    <LinearLayout
        android:id="@+id/linearLayout"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        tools:layout_editor_absoluteX="0dp"
        tools:layout_editor_absoluteY="84dp">

        <Button
            android:id="@+id/btn_private"
            style="?android:attr/buttonBarButtonStyle"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginTop="30dp"
            android:layout_marginBottom="10dp"
            android:layout_weight="0.5"
            android:background="#00FFFFFF"
            android:drawableTop="@drawable/ic_baseline_home_24"
            android:text="@string/privat"
            android:textColor="@color/black"
            tools:layout_editor_absoluteX="502dp"
            tools:layout_editor_absoluteY="80dp" />

        <Button
            android:id="@+id/btn_business"
            style="?android:attr/buttonBarButtonStyle"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginTop="30dp"
            android:layout_marginBottom="10dp"
            android:layout_weight="0.5"
            android:background="#00FFFFFF"
            android:drawableTop="@drawable/ic_baseline_business_center_24"
            android:text="@string/business"
            android:textColor="@color/black"
            tools:layout_editor_absoluteX="207dp"
            tools:layout_editor_absoluteY="80dp" />
    </LinearLayout>


</androidx.constraintlayout.widget.ConstraintLayout>