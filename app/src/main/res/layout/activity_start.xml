<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Admin.Start">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        tools:layout_editor_absoluteX="0dp"
        tools:layout_editor_absoluteY="0dp">

        <Button
            android:id="@+id/btn_private"
            android:layout_width="0dp"
            android:layout_weight="0.5"
            android:layout_height="wrap_content"
            android:layout_marginTop="30dp"
            android:layout_marginBottom="10dp"
            android:background="#00FFFFFF"
            android:drawableTop="@drawable/ic_baseline_home_24"
            android:text="@string/privat"
            android:textColor="@color/black"
            tools:layout_editor_absoluteX="502dp"
            tools:layout_editor_absoluteY="80dp"
            style="?android:attr/buttonBarButtonStyle" />

        <Button
            android:id="@+id/btn_business"
            android:layout_width="0dp"
            android:layout_weight="0.5"
            android:layout_height="wrap_content"
            android:layout_marginTop="30dp"
            android:layout_marginBottom="10dp"
            android:background="#00FFFFFF"
            android:drawableTop="@drawable/ic_baseline_business_center_24"
            android:text="@string/business"
            android:textColor="@color/black"
            tools:layout_editor_absoluteX="207dp"
            tools:layout_editor_absoluteY="80dp"
            style="?android:attr/buttonBarButtonStyle" />
    </LinearLayout>

    <LinearLayout
        android:layout_width="391dp"
        android:layout_height="568dp"
        android:orientation="vertical"
        tools:layout_editor_absoluteX="0dp"
        tools:layout_editor_absoluteY="111dp">

        <View
            android:layout_width="match_parent"
            android:layout_height="2dp"
            android:background="#000000" />

        <TextView
            android:id="@+id/tv_showbills"
            android:layout_width="match_parent"
            android:layout_height="11dp" />

    </LinearLayout>


</androidx.constraintlayout.widget.ConstraintLayout>